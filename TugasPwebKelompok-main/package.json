{
  "name": "e-transport-backend",
  "version": "1.0.0",
  "description": "Backend API for E-Transport train booking application",
  "main": "src/server.js",
  "scripts": {
    "start": "npx prisma generate && npx prisma db push && node prisma/seed.js && node src/server.js",
    "dev": "nodemon src/server.js",
    "seed": "node prisma/seed.js",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed"
  },
  "keywords": [
    "express",
    "prisma",
    "train",
    "booking"
  ],
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "@prisma/client": "^5.21.1",
    "prisma": "^5.21.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}